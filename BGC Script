	Callback = function(Value)
		_G.frostedegg = Value
		frostedegg()
	end     
})
--Notification
OrionLib:MakeNotification({
	Name = "AUTOSELL, AUTOBUY - IMPORTANT",
	Content = "!!!!!!!!!!!!You must be near the area for it to work!!!!!!!!!!!",
	Image = "rbxassetid://4483345998",
	Time = 10
})


OrionLib:Init()
