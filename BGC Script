--Tabs
local MainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local EggsTab = Window:MakeTab({
	Name = "Eggs",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
-- Toggles
MainTab:AddToggle({
	Name = "AutoBubble",
	Default = false,
	Callback = function(Value)
		_G.autobubble = Value
		autobubble()
	end     
})

MainTab:AddToggle({
	Name = "AutoSell",
	Default = false,
	Callback = function(Value)
		_G.autosell = Value
		autosell()
	end     
})

local CreditTab = Window:MakeTab({
	Name = "Credit",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
--Selection
local Section = EggsTab:AddSection({
	Name = "Auto Buy Eggs"
})

local Section = CreditTab:AddSection({
	Name = "Credit- Unggoy#3120"
})

--[[
Name = <string> - The name of the section.
]]

EggsTab:AddToggle({
	Name = "CommonEgg",
	Default = false,
	Callback = function(Value)
		_G.commonegg = Value
		commonegg()
	end     
})

EggsTab:AddToggle({
	Name = "SpottedEgg",
	Default = false,
	Callback = function(Value)
		_G.spottedegg = Value
		spottedegg()
	end     
})

EggsTab:AddToggle({
